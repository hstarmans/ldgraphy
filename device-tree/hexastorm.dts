/dts-v1/;
/plugin/;

/ {
	compatible = "ti,beaglebone", "ti,beaglebone-black";

	/* identification */
	part-number = "pinctrl-test-0";

	fragment@0 {
		target = <&am33xx_pinmux>;
		__overlay__ {
			pinctrl_test: pinctrl_test_0_pins {
				pinctrl-single,pins = <
					0x1ac 0x05  /* P9.25 Polygon Enable */
					0x1a4 0x05  /* P9.27 Polygon PWM    */
                                        0x194 0x05  /* P9.29 Laserdiode     */
				        0x1a8 0x26  /* P9.41 Photodiode     */
                                        0x178 0x07  /* P9.20 Y-dir          */
                                        0x078 0x07  /* P9.12 X-dir          */
					0x19c 0x05  /* P9.28 X-step         */
					0x198 0x05  /* P9.30 Y-step         */
                                        0x048 0x27  /* P9.14 X-endstop      */
					0x04c 0x27  /* P9.16 Y-endstop      */
				>;
			};
		};
	};

	fragment@1 {
		target = <&ocp>;
		__overlay__ {
			test_helper: helper {
				compatible = "bone-pinmux-helper";
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_test>;
				status = "okay";
			};
		};
	};
};
